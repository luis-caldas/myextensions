{
    "manifest_version": 2,
    "name": "Clear Start",
    "short_name": "Clear Start",
    "description": "Clears browsing data on exit / startup",
    "version": "0.0.1",
    "minimum_chrome_version": "72",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png",
        "256": "icons/icon-256.png",
        "512": "icons/icon-512.png"
    },
    "browser_action": {
        "default_title": "Clear Start"
    },
    "background": {
        "persistent": false,
        "scripts": [ "background.js" ]
    },
    "options_page": "options.html",
    "permissions": [ "tabs", "storage", "cookies", "browsingData", "contentSettings", "<all_urls>" ],
    "web_accessible_resources": [ "*" ],
    "offline_enabled": true
}
